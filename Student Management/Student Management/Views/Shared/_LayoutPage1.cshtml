@using System.Web.Optimization;
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Security</title>
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <link rel="icon" type="image/png" href="~/AdminLTE/dist/img/DIMS-logo.png" /> 
    <link href="~/Content/img/spinner.css" rel="stylesheet" />
    @Styles.Render("~/AdminLTE/plugins/font-awesome/css/font-awesome.min.css")
    @Styles.Render("~/AdminLTE/plugins/ionicons/css/ionicons.min.css")
    @Styles.Render("~/AdminLTE/bootstrap/css/bootstrap.min.css")
    @Styles.Render("~/AdminLTE/dist/css/admin-lte.min.css")
    @Styles.Render("~/AdminLTE/dist/css/skins/_all-skins.min.css")

    @Scripts.Render("~/bundles/custom")

    @Scripts.Render("~/AdminLTE/plugins/jquery/js/jQuery-2.1.4.min.js")
    @RenderSection("menu", required: false)
    @Scripts.Render("~/AdminLTE/bootstrap/js/bootstrap.min.js")
    @Scripts.Render("~/AdminLTE/plugins/slimscroll/js/jquery.slimscroll.min.js")
    @Scripts.Render("~/AdminLTE/plugins/fastclick/js/fastclick.min.js")
    @Scripts.Render("~/Scripts/Shared/_Layout.js")
    @Scripts.Render("~/AdminLTE/dist/js/app.min.js")
    @RenderSection("scripts", required: false)


</head>

<body class="hold-transition skin-blue sidebar-mini">
    <div id="bodyContent">
        <div class="wrapper">
            <header class="main-header">
                <a class="logo">
                    <span class="logo-lg"><b>F&O</b></span>
                </a>
                <nav class="navbar navbar-static-top" role="navigation">
                    <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button">
                        <span class="sr-only">Toggle navigation</span>
                    </a>
                    <div class="navbar-custom-menu">
                        <ul class="nav navbar-nav">
                            <li>
                                <a href="@Url.Action("Index","Login")">
                                    <i class="fa fa-sign-out"></i> Logout
                                </a>
                            </li>
                        </ul>
                    </div>
                </nav>
            </header>
            <aside class="main-sidebar">
                <section class="sidebar">
                    <div class="user-panel">
                        <div class="pull-left image">

            @*<img src="~/AdminLTE/dist/img/Dims.png" class="img-circle" alt="User Image" />*@

                        </div>
                        <div class="pull-left info">
                            <p>
                            </p>
                        </div>

                    </div>

                    <ul class="sidebar-menu">
                        <li class="header">MAIN NAVIGATION</li>
                        @*<li id="left-sidebar-menu-dashboard">
                            <a href="@Url.Action("CreateStudent","Student")">
                                <i class="fa fa-dashboard"></i> <span>Add Student</span>
                            </a>
                        </li>

                        <li id="left-sidebar-menu-forms">
                            <a href="@Url.Action("Index","StudentList")">
                                <i class="fa fa-book"></i> <span>Student's List</span>
                            </a>
                        </li>*@
                        <li id="left-sidebar-menu-charts" class="treeview">
                            <a href="#">
                                <i class="fa fa-pie-chart"></i>
                                <span>Users</span>
                                <i class="fa fa-angle-left pull-right"></i>
                            </a>
                            <ul class="treeview-menu">
                                <li><a href="#"><i class="fa fa-circle-o"></i>Users List</a></li>
                                <li><a href="#"><i class="fa fa-circle-o"></i>Create User</a></li>
                            </ul>
                        </li>

                        <li id="left-sidebar-menu-charts" class="treeview">
                            <a href="#">
                                <i class="fa fa-pie-chart"></i>
                                <span>Admin</span>
                                <i class="fa fa-angle-left pull-right"></i>
                            </a>
                            <ul class="treeview-menu">
                                <li><a href="#"><i class="fa fa-circle-o"></i>Companies</a></li>
                                
                            </ul>
                        </li>
                        @*<li id="left-sidebar-menu-forms">
                            <a href="#">
                                <i class="fa fa-book"></i><span>Issue Book</span>
                            </a>
                        </li>
                        <li id="left-sidebar-menu-forms">
                            <a href="#">
                                <i class="fa fa-book"></i><span>Return Return</span>
                            </a>
                        </li>*@

                    </ul>
                </section>
            </aside>
            <div class="content-wrapper">
                <section class="content">

                    @RenderBody()
                </section>
            </div>
            <footer class="main-footer">
                <strong>Copyrights |<a href="#">Friends and Company</a>.</strong> All rights
                reserved.
            </footer>

        </div>
        <div class="modal fade" id="myModal" role="dialog">
            <div class="modal-dialog">

                <!-- Modal content-->
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal">&times;</button>

                    </div>
                    <div class="modal-body" id="divModalBody">

                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</body>
</html>
